{
  "name": "benchmark-bs",
  "version": "1.0.0",
  "description": "A benchmarking application for bucklescript",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "re:build": "rescript build -with-deps",
    "re:watch": "rescript build -w",
    "re:clean": "rescript clean",
    "dev": "parcel src/index.html",
    "build": "yarn re:build && parcel build src/index.html --public-url ./",
    "deploy": "yarn build && gh-pages -d dist"
  },
  "author": "John",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "gh-pages": "^3.1.0",
    "parcel": "^1.12.4",
    "rescript": "^9.1.2",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^22.0.0"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "mori": "^0.3.2",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reason-react": "0.9.1",
    "rescript-vector": "0.2.0"
  }
}
